

    
    <table>
        <tr>
            <th>Firstname</th>
            <th>Lasname</th>
            <th>Email</th>
            <th>Action</th>
        </tr>
        <% @users.each do |user| %>
        <tr>
            <td><%= user.fname %></td>
            <td><%= user.lname %></td>
            <td><%= user.email %></td>
            <td>
                <%= link_to "Show", user_path(user) %>
                |
                <%= link_to "Delete", delete_user_path(user) %>
            </td>
        </tr>
        <% end %>
    </table>


    
    

