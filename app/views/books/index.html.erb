
<table>
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Author</th>
        <th>ISBN</th>
        <th>Created Date</th>
    </tr>
    <% @books.each do |book| %>
    <tr>
        <td><%= book.id %></td>
        <td><%= book.title %></td>
        <td><%= book.author %></td>
        <td><%= book.isbn %></td>
        <td><%= book.created_at.strftime("%m/%d/%y") %></td>
    </tr>
    <% end %>
</table>
